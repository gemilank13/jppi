<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="sequence_self_name" model="ir.sequence">
		   <field name="name">Self Name</field>
		   <field name="code">self.name</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">PM/%(day)s%(month)s%(y)s/</field>
		   <field name="padding">3</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>

		<record id="sequence_self_repair" model="ir.sequence">
		   <field name="name">Self Repair</field>
		   <field name="code">self.repair</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">REP/%(day)s%(month)s%(y)s/</field>
		   <field name="padding">3</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>
	</data>
</odoo>